<%- include('../partials/header') %>
    <!-- Form content -->
    <section class="support-container">
      <h3>Contact Us</h3>
      <form id="contact-form">
        <label for="name">Name:</label><br />
        <input type="text" id="name" name="name" required /><br /><br />

        <label for="email">Email:</label><br />
        <input type="email" id="email" name="email" required /><br /><br />

        <label for="subject">Subject:</label><br />
        <input type="text" id="subject" name="subject" required /><br /><br />

        <label for="message">Message:</label><br />
        <textarea id="message" name="message" rows="5" required></textarea><br /><br />

        <button type="submit">Submit</button>
      </form>
    </section>
    <!-- faq section -->
    <section class="faq-section">
      <h3>Frequently Asked Questions</h3>

      <div class="faq-item">
        <button class="faq-question" aria-expanded="false">
          What is the minimum age to rent a car?
        </button>
        <div class="faq-answer" hidden>
          You must be at least 21 years old with a valid driverâ€™s license to rent a car.
        </div>
      </div>

      <div class="faq-item">
        <button class="faq-question" aria-expanded="false">
          Can I rent a car without a credit card?
        </button>
        <div class="faq-answer" hidden>
          Most rentals require a credit card for security, but some locations accept debit cards with restrictions.
        </div>
      </div>

      <div class="faq-item">
        <button class="faq-question" aria-expanded="false">
          Is there a mileage limit on rentals?
        </button>
        <div class="faq-answer" hidden>
          Mileage limits depend on the rental agreement; some cars offer unlimited mileage.
        </div>
      </div>
    </section>
